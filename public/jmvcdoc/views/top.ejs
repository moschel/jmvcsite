<div class='top'>
	<div class="topCorner">
		<div class="right"></div>
		<div class="left"></div>
	</div>
	<div class="content">
	    <h1><%=this.title || name.replace(/~/g,".") %>&nbsp;
	    	<span class='<%= type%> type'><span class="typeEnd"><%= type %></span></span>&nbsp;
	    	<span class="favorite favorite<%= isFavorite? 'on' : 'off'%>">&nbsp;&nbsp;&nbsp;</span></h1>
	    <% if(this.inherits){ %>
	    <div class='inherits'>
	        inherits: <%= linkOpen(this.inherits) %>
	    </div>
	    <%} %>
	    <% if(this.tags){ %>
	    <div class='tags'>
	        tags: <%= linkTags(this.tags) %>
	    </div>
	    <%} %>
	    <% if(this.plugin){ %>
	    <div class='plugin'>
	        plugin: <%= this.plugin %>
	    </div>
	    <% } %>
		<% if(this.download){ %>
	    <div class='download'>
	        download: <a href='<%= steal.root? steal.root.join(this.download): this.download %>'><%=this.download.match(/[^\/]*$/)[0]%></a>
	    </div>
	    <% } %>
		<% if(this.test){ %>
	    <div class='test'>
	        test: <a href='<%= steal.root? steal.root.join(this.test): this.test %>'><%=this.test.match(/[^\/]*$/)[0]%></a>
	    </div>
	    <% } %>
	</div>
	<div class="bottomCorner">
		<div class="right"></div>
		<div class="left"></div>
	</div>	
</div>
