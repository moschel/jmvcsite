<div class="iframe_menu_wrapper">
	<ul class="iframe_menu">		
	    <li class="iframe_menu_item">
			<a href="javascript://" data-src="<%= iframeWindow.location %>.js">
    		    <%= shortenUrl(iframeWindow.location) %>
		    </a>
		</li>					
	<% for(var s=0;s<scripts.length;s++) {
	    var script = scripts[s]; %>
	    <li class="iframe_menu_item">
	    	<% if(script.src) { %>
    			<a href="javascript://"  data-src="<%= script.src %>">
	    		    <%= shortenUrl(script.src) %>
		    <% } %>
		    </a>
		</li>	
	<% } %>
	</ul>
</div>

